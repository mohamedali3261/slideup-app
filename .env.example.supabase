# Supabase PostgreSQL Configuration
# Copy this file to .env and fill in your values

# Database Connection (from Supabase Dashboard → Settings → Database)
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.[PROJECT-REF].supabase.co:5432/postgres

# JWT Secret (generate a strong random string)
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# CORS Origin (your Netlify site URL)
CORS_ORIGIN=https://your-site-name.netlify.app

# Node Environment
NODE_ENV=production

# Admin Password (change after first login!)
ADMIN_PASSWORD=01021303309

# Optional: Supabase API Keys (if using Supabase client features)
# SUPABASE_URL=https://[PROJECT-REF].supabase.co
# SUPABASE_ANON_KEY=your-anon-key
# SUPABASE_SERVICE_KEY=your-service-key
